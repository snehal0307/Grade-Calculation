#include<stdio.h>
int main()
{
    int marks;
    printf("Enter your marks:");
    scanf("%d",&marks);

    if(marks>=85 && marks<=100)
    {
        printf("You got Grade A");

    }
    else if(marks>=70 && marks<=84)
    {
        printf("You got Grade B");
    }

    else if(marks>=55 && marks<=69)
    {
        printf("You got Grade C");
    }
    else if(marks<40)
    {
        printf("You got Grade F");

    }
    return 0;
}
